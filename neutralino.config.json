{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "js.neutralino.reactnative",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/vite-dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log"
  ],
  "modes": {
    "window": {
      "title": "React Native + Neutralinojs",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "enableInspector": false,
      "openInspectorOnStartup": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    },
    "browser": {
      "nativeBlockList": ["filesystem.*"]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": ["filesystem.*", "os.*"]
    }
  },
  "cli": {
    "binaryName": "neutralino",
    "resourcesPath": "/vite-dist/",
    "extensionsPath": "/extensions/"
  }
}